{
  "name": "imbo/imbo-metadata-cache",
  "type": "library",
  "description": "Metadata cache event listener for Imbo",
  "homepage": "http://www.imbo.io/",
  "license": "MIT",
  "authors": [
    {
      "name": "Christer Edvartsen",
      "email": "cogo@starzinger.net",
      "role": "Project lead",
      "homepage": "http://cogo.wordpress.com"
    }
  ],
  "support": {
    "issues": "https://github.com/imbo/imbo/issues",
    "irc": "irc://irc.freenode.net/imbo"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:imbo/imbo.git"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "imbo/imbo": "dev-imbo-3.0",
    "phpunit/phpunit": "~4.8",
    "ext-memcached": "~2.1.0",
    "ext-apcu": "~4.0.0"
  },
  "suggest": {
    "ext-memcached": "Enables usage of the Memcached cache adapter. Recommended version: >=2.2.0",
    "ext-apcu": "Enables usage of the APCu cache adapter. Recommended version: >=5.1.3"
  },
  "autoload": {
    "psr-4": {
      "Imbo\\Plugin\\MetadataCache\\": "src/",
      "ImboTest\\Plugin\\MetadataCache\\": "tests/"
    }
  }
}
